"UnlitGeneric"
{
    "$basetexture" "tbrick/shuffle"
	"$translucent" 1
	"$vertexalpha" 1
	"$vertexcolor" 1
    "Proxies"
        {
            "AnimatedOffsetTexture"
            {
                "animatedtexturevar" "$basetexture"
                "animatedtextureframenumvar" "$frame"
                "animatedtextureframerate" 8.0
            }
}
}